{
  "title": "gy",
  "rules": [
    {
      "description": "[访达] Delete => 移至废纸篓",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "delete_forward",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "delete_or_backspace",
              "modifiers": ["left_command"]
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": ["^com.apple.finder"]
            }
          ]
        }
      ]
    },
    {
      "description": "[访达] Backspace => 按下backspace,按住上层文件夹",
      "manipulators": [
        {
          "from": {
            "key_code": "delete_or_backspace"
          },
          "parameters": {
            "basic.to_if_alone_timeout_milliseconds": 200,
            "basic.to_if_held_down_threshold_milliseconds": 200
          },
          "to_if_alone": [
            {
              "key_code": "delete_or_backspace"
            }
          ],
          "to_if_held_down": [
            {
              "key_code": "up_arrow",
              "modifiers": ["left_command"]
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": ["^com.apple.finder"]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "[浏览器] F5 => 刷新",
      "manipulators": [
        {
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^org\\.mozilla\\.firefox$",
                "^com\\.microsoft\\.edgemac",
                "^com\\.google\\.Chrome$",
                "^com\\.apple\\.Safari$"
              ]
            }
          ],
          "from": {
            "key_code": "f5",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "r",
              "modifiers": ["left_command"]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "alt+i/j/k/l => 上/左/下/右",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "j",
            "modifiers": {
              "mandatory": ["left_option"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "left_arrow"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "k",
            "modifiers": {
              "mandatory": ["left_option"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "down_arrow"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "i",
            "modifiers": {
              "mandatory": ["left_option"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "up_arrow"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "l",
            "modifiers": {
              "mandatory": ["left_option"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "right_arrow"
            }
          ]
        }
      ]
    },
    {
      "description": "alt+tab/` => 切换程序/窗口",
      "manipulators": [
        {
          "from": {
            "key_code": "tab",
            "modifiers": {
              "mandatory": ["left_option"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "tab",
              "modifiers": ["left_command"]
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "grave_accent_and_tilde",
            "modifiers": {
              "mandatory": ["left_option"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "grave_accent_and_tilde",
              "modifiers": ["left_command"]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "[vscode] alt+space => 显示所有命令(cmd+shift+p)",
      "manipulators": [
        {
          "conditions": [
            {
              "bundle_identifiers": [
                "com\\.microsoft\\.VSCode",
                "com\\.microsoft\\.VSCodeInsiders"
              ],
              "type": "frontmost_application_if"
            }
          ],
          "from": {
            "key_code": "spacebar",
            "modifiers": {
              "mandatory": ["option"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "p",
              "modifiers": ["left_command", "left_shift"]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "f1 => 轻按alfred,按住siri (需配置快捷键)",
      "manipulators": [
        {
          "from": {
            "key_code": "f1"
          },
          "parameters": {
            "basic.to_if_alone_timeout_milliseconds": 200,
            "basic.to_if_held_down_threshold_milliseconds": 200
          },
          "to_if_alone": [
            {
              "key_code": "f1",
              "modifiers": [
                "left_command",
                "left_shift",
                "left_control",
                "left_option"
              ]
            }
          ],
          "to_if_held_down": [
            {
              "key_code": "f2",
              "modifiers": [
                "left_command",
                "left_shift",
                "left_control",
                "left_option"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "f2 => 轻按为调度中心,按住显示桌面",
      "manipulators": [
        {
          "from": {
            "key_code": "f2"
          },
          "parameters": {
            "basic.to_if_alone_timeout_milliseconds": 200,
            "basic.to_if_held_down_threshold_milliseconds": 200
          },
          "to_if_alone": [
            {
              "key_code": "up_arrow",
              "modifiers": ["left_control"]
            }
          ],
          "to_if_held_down": [
            {
              "key_code": "f11"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "f4 => 轻按缩放,按住最大化 (需配置快捷键)",
      "manipulators": [
        {
          "from": {
            "key_code": "f4"
          },
          "parameters": {
            "basic.to_if_alone_timeout_milliseconds": 200,
            "basic.to_if_held_down_threshold_milliseconds": 200
          },
          "to_if_alone": [
            {
              "key_code": "s",
              "modifiers": ["left_control", "left_command"]
            }
          ],
          "to_if_held_down": [
            {
              "key_code": "f",
              "modifiers": ["left_control", "left_command"]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "tab+a/s/d => 窗口停靠(按下)/拼贴(按住)到屏幕左/右以及复原 (需配置快捷键)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "tab",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "set_variable": {
                "name": "tabfn_mode",
                "value": 1
              }
            }
          ],
          "to_if_alone": [
            {
              "key_code": "tab"
            }
          ],
          "to_after_key_up": [
            {
              "set_variable": {
                "name": "tabfn_mode",
                "value": 0
              }
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "a",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "parameters": {
            "basic.to_if_alone_timeout_milliseconds": 250,
            "basic.to_if_held_down_threshold_milliseconds": 250
          },
          "to_if_alone": [
            {
              "key_code": "f9",
              "modifiers": ["left_command", "left_control"]
            }
          ],
          "to_if_held_down": [
            {
              "key_code": "f9",
              "modifiers": ["left_command", "left_option", "left_control"]
            }
          ],
          "conditions": [
            {
              "type": "variable_if",
              "name": "tabfn_mode",
              "value": 1
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "s",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to_if_alone": [
            {
              "key_code": "f10",
              "modifiers": ["left_command", "left_control"]
            }
          ],
          "to_if_held_down": [
            {
              "key_code": "f10",
              "modifiers": ["left_command", "left_option", "left_control"]
            }
          ],
          "conditions": [
            {
              "type": "variable_if",
              "name": "tabfn_mode",
              "value": 1
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "d",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to_if_alone": [
            {
              "key_code": "f11",
              "modifiers": ["left_command", "left_option", "left_control"]
            }
          ],
          "conditions": [
            {
              "type": "variable_if",
              "name": "tabfn_mode",
              "value": 1
            }
          ]
        }
      ]
    },
    {
      "description": "tab+ 1/2/3/4 & q/w & space => 切换桌面1/2/3/4 & 向左/右移动桌面空间 & 按下调度中心/按住显示桌面",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "tab",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "set_variable": {
                "name": "tabfn_mode",
                "value": 1
              }
            }
          ],
          "to_if_alone": [
            {
              "key_code": "tab"
            }
          ],
          "to_after_key_up": [
            {
              "set_variable": {
                "name": "tabfn_mode",
                "value": 0
              }
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "1"
          },
          "to_if_alone": [
            {
              "key_code": "1",
              "modifiers": ["left_control"]
            }
          ],
          "conditions": [
            {
              "type": "variable_if",
              "name": "tabfn_mode",
              "value": 1
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "2"
          },
          "to_if_alone": [
            {
              "key_code": "2",
              "modifiers": ["left_control"]
            }
          ],
          "conditions": [
            {
              "type": "variable_if",
              "name": "tabfn_mode",
              "value": 1
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "3"
          },
          "to_if_alone": [
            {
              "key_code": "3",
              "modifiers": ["left_control"]
            }
          ],
          "conditions": [
            {
              "type": "variable_if",
              "name": "tabfn_mode",
              "value": 1
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "4"
          },
          "to_if_alone": [
            {
              "key_code": "4",
              "modifiers": ["left_control"]
            }
          ],
          "conditions": [
            {
              "type": "variable_if",
              "name": "tabfn_mode",
              "value": 1
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "q"
          },
          "to_if_alone": [
            {
              "key_code": "left_arrow",
              "modifiers": ["left_control"]
            }
          ],
          "conditions": [
            {
              "type": "variable_if",
              "name": "tabfn_mode",
              "value": 1
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "w"
          },
          "to_if_alone": [
            {
              "key_code": "right_arrow",
              "modifiers": ["left_control"]
            }
          ],
          "conditions": [
            {
              "type": "variable_if",
              "name": "tabfn_mode",
              "value": 1
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "spacebar"
          },
          "parameters": {
            "basic.to_if_alone_timeout_milliseconds": 200,
            "basic.to_if_held_down_threshold_milliseconds": 200
          },
          "to_if_alone": [
            {
              "key_code": "up_arrow",
              "modifiers": ["left_control"]
            }
          ],
          "to_if_held_down": [
            {
              "key_code": "f11"
            }
          ],
          "conditions": [
            {
              "type": "variable_if",
              "name": "tabfn_mode",
              "value": 1
            }
          ]
        }
      ]
    }
  ]
}
